<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".dashboard">


    <TextView
        android:id="@+id/labelAtas"
        android:layout_width="wrap_content"
        android:layout_height="26dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="17dp"
        android:text="PILIH MENU
"
        android:textSize="18sp" />

    <LinearLayout
        android:id="@+id/Menu1"
        android:layout_width="match_parent"
        android:layout_height="117dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="62dp"
        android:orientation="vertical">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="110dp">

            <Button
                android:id="@+id/tbh_Biodata"
                android:layout_width="98dp"
                android:layout_height="95dp"
                android:layout_alignParentStart="true"
                android:layout_centerVertical="true"
                android:layout_marginStart="20dp"
                android:background="@drawable/logodepan" />

            <TextView
                android:id="@+id/tv_biodata"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignTop="@+id/tbh_Biodata"
                android:layout_marginStart="130dp"
                android:text="Profil"
                android:textSize="17sp" />
        </RelativeLayout>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/Menu2"
        android:layout_width="match_parent"
        android:layout_height="117dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="186dp"
        android:orientation="vertical">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="110dp">

            <Button
                android:id="@+id/tbh_Dosen"
                android:layout_width="98dp"
                android:layout_height="95dp"
                android:layout_alignParentStart="true"
                android:layout_centerVertical="true"
                android:layout_marginStart="20dp"
                android:background="@drawable/logodepan" />

            <TextView
                android:id="@+id/tv_dosen"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignTop="@+id/tbh_Dosen"
                android:layout_marginStart="130dp"
                android:text="Menu 2"
                android:textSize="17sp" />
        </RelativeLayout>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/Menu3"
        android:layout_width="match_parent"
        android:layout_height="117dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="230dp"
        android:orientation="vertical">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="110dp">

            <Button
                android:id="@+id/tbh_Jadwal"
                android:layout_width="98dp"
                android:layout_height="95dp"
                android:layout_alignParentStart="true"
                android:layout_centerVertical="true"
                android:layout_marginStart="20dp"
                android:background="@drawable/logodepan" />

            <TextView
                android:id="@+id/tv_jadwal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignTop="@+id/tbh_Jadwal"
                android:layout_marginStart="131dp"
                android:text="Menu 3"
                android:textSize="17sp" />
        </RelativeLayout>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/Menu4"
        android:layout_width="match_parent"
        android:layout_height="117dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="106dp"
        android:orientation="vertical">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="110dp">

            <Button
                android:id="@+id/document"
                android:layout_width="98dp"
                android:layout_height="95dp"
                android:layout_alignParentStart="true"
                android:layout_centerVertical="true"
                android:layout_marginStart="20dp"
                android:background="@drawable/logodepan" />

            <TextView
                android:id="@+id/tv_document"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignTop="@+id/document"
                android:layout_marginStart="131dp"
                android:text="Menu 4"
                android:textSize="17sp" />
        </RelativeLayout>

    </LinearLayout>

</RelativeLayout>